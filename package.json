{
  "name": "lambda_generator",
  "version": "1.0.0",
  "description": "A scaffolding to generate lambda typeScript Projects",
  "main": "./build/index.js",
  "module": "commonjs",
  "scripts": {
    "debug": "npx nodemon ./debugServer.ts localhost 8080",
    "build:prod": "rimraf ./bundle && npm run build:tsc && node esbuild.config.js",
    "build:tsc": "rimraf ./build && npx tsc",
    "test": "jest",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "keywords": [
    "yeoman-generator"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@types/aws-lambda": "^8.10.107",
    "@types/jest": "^29.1.2",
    "@types/node": "^14.18.32",
    "esbuild": "^0.14.0",
    "fastify": "^4.9.2",
    "gts": "^3.1.1",
    "jest": "^29.2.0",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "ts-auto-mock": "^3.6.2",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "ttypescript": "^1.5.13",
    "typescript": "~4.7.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.190.0",
    "@aws-sdk/lib-dynamodb": "^3.190.0",
    "@typedorm/common": "^1.15.0",
    "@typedorm/core": "^1.15.0",
    "aws-lambda": "^1.0.7",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0"
  }
}
